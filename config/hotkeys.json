{
  "focus": {
    "Ctrl+1": "instance-0",
    "Ctrl+2": "instance-1",
    "Ctrl+3": "instance-2",
    "Ctrl+4": "instance-3",
    "Ctrl+5": "instance-4",
    "Ctrl+6": "instance-5",
    "Ctrl+7": "instance-6",
    "Ctrl+8": "instance-7",
    "Ctrl+9": "instance-8"
  },
  "zoom": {
    "Ctrl=": "zoom-in",
    "Ctrl-": "zoom-out"
  },
  "switch": {
    "Tab": "next-instance"
  }
}
